<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="2">

	<groups>
		<run>
			<include name="errorHandling"></include>
		</run>
	</groups>
	<listeners>
		<listener class-name="qaClick.TestComponents.Listeners"></listener>
	</listeners>

	<test thread-count="5" name="Submit Order Test">
		<classes>
			<class name="qaClick.Tests.OrderPlacingTest" />
		</classes>
	</test> <!--
	Test -->
	<test thread-count="5" name="Error Validation Test">
		<classes>
			<class name="qaClick.Tests.ErrorValidationTest" />
		</classes>
	</test>
</suite> <!--
Suite -->
